<%= form_tag sessions_path do %>
      <h1> Login </h1>
      <p>
        <%= label_tag 'email' %><br />
        <%= text_field_tag 'email' %>
      </p>
      <p>
        <%= label_tag 'password' %><br />
        <%= password_field_tag 'password' %>
      </p>
      <p>
        <%= submit_tag 'Login' %>
      </p>
<% end %>